{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cargo",
      "command": "build",
      "problemMatcher": ["$rustc"],
      "group": "build",
      "label": "rust: cargo build"
    },
    {
      "type": "cargo",
      "command": "run",
      "problemMatcher": ["$rustc"],
      "group": "build",
      "label": "rust: cargo run dev",
      "options": {
        "env": {
          "DEV_TOOLS": "1"
        }
      }
    }
  ]
}
